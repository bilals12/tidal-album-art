{
  "name": "@tidal-music/album-art",
  "version": "0.1.0",
  "type": "module",
  "description": "Album art retrieval for TIDAL",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "vite build",
    "build": "vite build",
    "build:dev": "vite build -m development",
    "clean": "rm -rf coverage dist .eslintcache",
    "dev": "node --import tsx --import ./global-setup.mjs src/index.ts",
    "lint": "eslint . --cache --cache-strategy content",
    "lint:ci": "eslint . --quiet",
    "lint:fix": "pnpm run lint --fix",
    "preview": "vite preview",
    "test": "vitest --sequence.concurrent",
    "test:coverage": "pnpm run test --coverage",
    "test:ui": "pnpm run test:coverage --ui",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@tidal-music/auth": "workspace:^",
    "@tidal-music/catalogue": "workspace:^",
    "@tidal-music/player": "workspace:^",
    "sharp": "0.33.4"
  },
  "devDependencies": {
    "@types/sharp": "0.32.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "ts-node": "10.9.2",
    "tsx": "4.16.2",
    "typescript": "5.5.3",
    "vite": "5.3.2",
    "vite-node": "2.0.4",
    "vite-plugin-dts": "3.9.1",
    "vitest": "1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:tidal-music/tidal-sdk-web.git"
  },
  "license": "Apache-2.0"
}